{
  "name": "studyManage",
  "version": "1.0.0",
  "description": "基于 Egg + Vue + Webpack ＋ Element 单页面管理系统骨架",
  "scripts": {
    "dev": "egg-bin dev",
    "start": "easy build prod && egg-scripts start --title=studyManage --daemon --port=3002",
    "restart": "egg-scripts stop --title=studyManage && easy build prod && egg-scripts start --title=studyManage --port=3002 --daemon",
    "stop": "egg-scripts stop --title=studyManage",
    "debug": "egg-bin debug",
    "clean": "easy clean",
    "build": "easy build ",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "ii": "npm install --registry https://registry.npm.taobao.org"
  },
  "dependencies": {
    "cross-env": "^5.0.0",
    "egg": "^2.1.0",
    "egg-bin": "^4.3.7",
    "egg-cors": "^2.0.0",
    "egg-logger": "^1.5.0",
    "egg-mongoose": "^3.3.1",
    "egg-mysql": "^3.1.0",
    "egg-scripts": "^2.9.1",
    "egg-validate": "^1.0.0",
    "extend": "~3.0.0",
    "fs-extra": "^10.0.1",
    "lodash": "^4.17.4",
    "lodash-id": "^0.14.0",
    "mockjs": "^1.0.1-beta3",
    "shortid": "^2.2.8",
    "showdown": "^2.0.3"
  },
  "devDependencies": {
    "autod-egg": "^1.0.0",
    "babel-plugin-import": "^1.13.3",
    "egg-logview": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-egg": "^5.0.0",
    "eslint-plugin-vue": "^4.4.0",
    "ip": "^1.1.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "ci": {
    "version": "8, 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easy-team/egg-vue-webpack-boilerplate.git"
  },
  "author": "hubcarl@126.com",
  "license": "MIT",
  "homepage": "https://github.com/easy-team/egg-vue-webpack-boilerplate",
  "webpack": {
    "loaders": {
      "scss": true
    }
  }
}
